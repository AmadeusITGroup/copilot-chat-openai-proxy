{
  "name": "chat-participant-openai-proxy",
  "displayName": "Copilot Chat OpenAI Dev Proxy",
  "version": "0.0.1",
  "description": "An extension that allows you start a little development-grade server on your machine(localhost) to proxy your OpenAI API compliant requests to models behind GitHub Copilot.",
  "publisher": "AmadeusITGroup",
  "author": {
    "name": "Amadeus IT Group",
    "email": "opensource@amadeus.com"
  },
  "icon": "icon.png",
  "keywords": [
    "ai",
    "assistant",
    "framework",
    "llm",
    "proxy",
    "openai",
    "productivity",
    "coding",
    "automation"
  ],
  "engines": {
    "vscode": "^1.95.1"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Education"
  ],
  "activationEvents": [],
  "extensionDependencies": [
    "github.copilot-chat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "chat-participant-openai-proxy.llmproxy",
        "name": "llmproxy",
        "description": "This participant allows you start a little development-grade server on your machine(localhost) to proxy your OpenAI API compliant requests to models behind GitHub Copilot",
        "isSticky": true,
        "commands": [
          {
            "name": "start",
            "description": "Starts the RestAPI server to proxy the requests to models behind Copilot Chat"
          }
        ]
      }
    ]
  },
  "configuration": {
    "title": "Copilot Chat OpenAI Dev Proxy",
    "properties": {
      "chat-participant-openai-proxy.llmproxy.hostname": {
        "type": "string",
        "default": "localhost",
        "description": "Hostname for the proxy server"
      },
      "chat-participant-openai-proxy.llmproxy.port": {
        "type": "number",
        "default": 8080,
        "description": "Port for the proxy server"
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "package:vsix": "vsce package",
    "compile-tests": "tsc -p tsconfig.test.json",
    "watch-tests": "tsc -p tsconfig.test.json -w",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "npm run test:integration",
    "test:unit": "mocha --require ./test/mocha.setup.js --require ./test/unit.setup.js test-dist/src/test/suite/*.test.js --ignore test-dist/src/test/suite/integration.test.js",
    "test:integration": "npm run compile-tests && node ./test/runExtensionTests.js",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:coverage": "nyc npm run test",
    "test:coverage:report": "nyc report --reporter=html --reporter=text --reporter=json",
    "test:coverage:unit": "nyc --reporter=html --reporter=text npm run test:unit",
    "test:coverage:integration": "nyc --reporter=html --reporter=text npm run test:integration",
    "coverage:clean": "rimraf coverage .nyc_output",
    "ignore:status": "node scripts/package-helpers.js status",
    "ignore:dev": "node scripts/package-helpers.js dev",
    "ignore:prod": "node scripts/package-helpers.js prod",
    "ignore:restore": "node scripts/package-helpers.js restore",
    "package:prepare": "cp -ar ./node_modules/swagger-ui-dist out/ && node scripts/package-helpers.js prod && npm run compile",
    "package:cleanup": "node scripts/package-helpers.js dev",
    "package:full": "npm run package:prepare && npm run package:vsix && npm run package:cleanup",
    "package:production": "npm run package:prepare && npm run package:vsix && npm run package:cleanup && echo \"ðŸ“¦ Production package created with optimized size\"",
    "dev:setup": "npm run ignore:dev && echo \"ðŸ”§ Development mode activated - Copilot friendly\"",
    "dev:package-test": "npm run package:prepare && echo \"ðŸ“‹ Switched to production mode - check file exclusions\" && npm run ignore:status"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/express": "^5.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonschema": "^1.1.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.14.13",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/vscode": "^1.95.0",
    "@types/yauzl": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/test-electron": "^2.2.0",
    "@vscode/vsce": "^2.31.1",
    "esbuild": "^0.25.11",
    "eslint": "^8.57.1",
    "fs-extra": "^11.3.2",
    "glob": "^11.1.0",
    "mocha": "^10.1.0",
    "nock": "^14.0.10",
    "nyc": "^17.1.0",
    "prettier": "^3.3.0",
    "prettier-eslint": "^16.3.0",
    "rimraf": "^6.0.1",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmadeusITGroup/copilot-chat-openai-proxy.git"
  },
  "bugs": {
    "url": "https://github.com/AmadeusITGroup/copilot-chat-openai-proxy/issues"
  },
  "homepage": "https://github.com/AmadeusITGroup/copilot-chat-openai-proxy#readme",
  "dependencies": {
    "axios": "^1.7.2",
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "jsonschema": "^1.5.0",
    "openai": "^4.53.1",
    "openapi-types": "^12.1.3",
    "swagger-ui-express": "^5.0.1"
  }
}
